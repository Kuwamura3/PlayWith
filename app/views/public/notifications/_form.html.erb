<%= form_with(url: notifications_path, method: :post, class: "form-inline", remote: true) do |f| %>
	<%= f.hidden_field :sender_id, value: user.id %>
	<%= f.hidden_field :game_deleted, value: "" %>
	<%= f.collection_select(:game_id, users_games, :id, :title, {}, {class: "form-control"}) %>
	<%= f.select :kind, {"を新しく始めました":"新規投稿", "で遊びたい":"希望投稿"}, {}, {class: "form-control"} %>
	<%= f.submit "投稿", class: "btn btn-md btn-info" %>
<% end %>